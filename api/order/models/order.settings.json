{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "Advert",
        "Coin_Promotion"
      ],
      "default": "Coin"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Completed",
        "Canceled"
      ],
      "default": "Pending"
    },
    "entityId": {
      "type": "string",
      "required": true
    },
    "orderItemText": {
      "type": "string",
      "required": true
    },
    "base_price": {
      "type": "float",
      "required": true
    },
    "discounted_price": {
      "type": "float",
      "required": true
    },
    "userNote": {
      "type": "text"
    },
    "created": {
      "type": "datetime"
    },
    "updated": {
      "type": "datetime"
    },
    "user": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
